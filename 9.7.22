def stage_check(r):
    print("checking", r[0][0], "against", r[1][0])
    for i in range(2): 
        for l in range(3):
            if int (r[i][l]) < int (r[i+1][l]):
                print("True")        
            else:
                print("False")
         
stage = ([
  [1, 2, 3],
  [4, 5, 6],
  [7, 8, 9]
  ])

stage_check(stage) #call the function
